<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="onNavigatingTo">
   
   
    <Page.actionBar>
        <ActionBar title="Cancel Edit">
            <NavigationButton android.systemIcon="ic_menu_back" text="Back to ActionBar" tap="backButtonClicked"></NavigationButton>
        </ActionBar>
    </Page.actionBar>
  
   
    <StackLayout>
        <TextField hint="First Name" text="{{ firstname }}" />
        <TextField hint="Last Name" text="{{ lastname }}" />
        <TextField hint="Age"/>
        <TextField hint="Distance Preference" text="{{ lastname }}" />
        <TextField hint="Gender" text="{{ firstname }}" />
        <StackLayout orientation="horizontal">
            <Button class="info-link-button5" text="Make Change" tap="backButtonClicked"  />
        </StackLayout>
    </StackLayout>
</Page>